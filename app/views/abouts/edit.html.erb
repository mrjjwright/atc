<h1>Admin for About</h1>

<% form_for(@about) do |f| %>
  <%= f.error_messages %>
 <p>
    <%= label_tag 'secret_key' %><br />
    <%= password_field_tag 'secret_key' %>
  </p>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>
